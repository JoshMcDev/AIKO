  "clauseSelectionEngine": {
    "rules": {
      "mandatoryInclusion": {
        "description": "These clauses must always be included based on contract characteristics",
        "logic": [
          {
            "condition": "contractValue > thresholds.microPurchase",
            "clauses": ["52.204-7", "52.222-40", "52.204-10"]
          },
          {
            "condition": "contractValue > thresholds.simplifiedAcquisition",
            "clauses": ["52.203-5", "52.203-6", "52.203-7", "52.219-8"]
          },
          {
            "condition": "contractType === 'COST_REIMBURSEMENT'",
            "clauses": ["52.216-7", "52.232-20", "52.232-22", "52.244-2"]
          },
          {
            "condition": "requirementType === 'CONSTRUCTION'",
            "clauses": ["52.236-2", "52.236-3", "52.236-5", "52.236-7", "52.236-13", "52.236-15"]
          },
          {
            "condition": "requirementType === 'SERVICES' && contractValue > 2500",
            "clauses": ["52.222-41"]
          },
          {
            "condition": "agency === 'DOD'",
            "clauses": ["252.204-7012", "252.232-7003"]
          }
        ]
      },
      "conditionalInclusion": {
        "description": "These clauses are included when specific conditions are met",
        "logic": [
          {
            "condition": "hasOptions === true",
            "clauses": ["52.217-8", "52.217-9"]
          },
          {
            "condition": "governmentPropertyProvided === true",
            "clauses": ["52.245-1", "52.245-9"]
          },
          {
            "condition": "internationalTransport === true",
            "clauses": ["52.247-63", "52.247-64"]
          },
          {
            "condition": "workPerformedInUS === true && contractValue > 150000 && performancePeriod > 120",
            "clauses": ["52.222-54"]
          },
          {
            "condition": "subcontractingExpected === true && contractValue > 750000 && !smallBusiness",
            "clauses": ["52.219-9"]
          },
          {
            "condition": "certifiedCostPricingDataRequired === true",
            "clauses": ["52.215-10", "52.215-12"]
          }
        ]
      },
      "commercialItemRules": {
        "description": "Special rules for commercial item acquisitions",
        "logic": [
          {
            "condition": "commercialItem === true",
            "includeBase": ["52.212-4", "52.212-5"],
            "excludeNonCommercial": ["52.203-3", "52.222-3", "52.215-2"]
          }
        ]
      }
    },
    "fillInProcessor": {
      "description": "Logic for processing clauses that require fill-in information",
      "templates": {
        "52.217-8": {
          "OPTION_PERIOD": "Derived from contract schedule",
          "NOTIFICATION_DAYS": "Default: 30 days unless specified"
        },
        "52.217-9": {
          "OPTION_PERIOD": "Derived from contract schedule",
          "NOTIFICATION_DAYS": "Default: 30 days unless specified"
        },
        "52.222-41": {
          "WAGE_DETERMINATION_NUMBER": "From www.wdol.gov based on location and service type"
        },
        "52.219-9": {
          "SUBCONTRACTING_GOALS": "Per agency goals and contract requirements"
        },
        "52.227-14": {
          "ALTERNATE_SELECTED": "Based on data rights requirements"
        }
      }
    },
    "provisionMapping": {
      "description": "Maps provisions to their corresponding contract clauses",
      "mappings": {
        "52.204-7": "Provision - include in solicitation",
